SELECT cd.Cdid, cd.Cdtitle, track.Trknum, track.Trktitle, track.Trklength
FROM cd
JOIN track ON cd.Cdid = track.Cdid
ORDER BY track.Trklength DESC;


SELECT cd.Cdid, cd.Cdtitle, track.Trknum, track.Trktitle, track.Trklength
FROM cd
JOIN track ON cd.Cdid = track.Cdid
WHERE track.Trklength >= 4 
AND track.Trklength <= 6
ORDER BY track.Trklength ASC;


SELECT cd.Cdid, cd.Cdtitle, SUM(track.Trklength) AS total_playing_time
FROM cd
JOIN track ON cd.Cdid = track.Cdid
GROUP BY cd.Cdid, cd.Cdtitle;


SELECT label.Lblnation AS Nation_or_Country, COUNT(cd.Cdid) AS Number_of_CDs
FROM label
JOIN cd ON label.Lbltitle = cd.Lbltitle
GROUP BY label.Lblnation;


SELECT label.Lbltitle AS Music_Label, COUNT(track.Trkid) AS Total_Tracks
FROM label
JOIN cd ON label.Lbltitle = cd.Lbltitle
JOIN track ON cd.Cdid = track.Cdid
GROUP BY label.Lbltitle
HAVING Total_Tracks < 10;


INSERT INTO cd (Cdid, Cdlblid, Cdtitle, Cdyear, Lbltitle)
VALUES (8, 'WD72645', 'Classical Hits', 1981, 'Motown');


